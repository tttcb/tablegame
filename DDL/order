-- .order definition

CREATE TABLE `order` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT COMMENT '主键id',
  `user_id` bigint(20) NOT NULL COMMENT '用户id',
  `address` varchar(100)  COMMENT '收货地址',
  `pay_way` varchar(100) DEFAULT '0' COMMENT '支付方式',
  `price` varchar(100) DEFAULT '0' COMMENT '价格',
  `date` date DEFAULT NULL COMMENT '交易时间',
  `status` tinyint(4) NOT NULL DEFAULT '0' COMMENT '删除状态（0未删除，1已删除）',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB  DEFAULT CHARSET=utf8;